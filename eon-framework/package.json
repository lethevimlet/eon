{
  "name": "eon",
  "version": "0.9.11",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@vimlet/meta": "2.0.8",
    "@vimlet/copy": "0.0.3",
    "npm-run-all": "4.1.3",
    "autoless": "0.1.7",
    "less": "3.8.1",
    "express": "4.16.4"
  },
  "scripts": {
    "build": "run-s meta copy less",
    "meta": "vimlet-meta -i src/meta/**/*.vmt -o build",
    "meta:watch": "vimlet-meta -i src/meta/**/*.vmt -o build -w src/meta",
    "copy": "vimlet-copy -i src/copy/**/* -o build",
    "copy:watch": "vimlet-copy -i src/copy/**/* -o build -w src/copy",
    "less": "node src/scripts/lessc.js src/less/**/*.less ../build/webapp",
    "less:watch": "node_modules/.bin/autoless src/less ../build/webapp",
    "start": "node src/server/index.js"
  }
}
