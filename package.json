{
  "name": "eon-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "postinstall": "run-s install:core install:ui",
    "install:core": "cd eon-core && npm i",
    "install:ui": "cd eon-ui && npm i",
    "build": "run-s build:core build:ui import-builds copy less meta",
    "build:core": "cd eon-core && npm run build",
    "build:ui": "cd eon-core && npm run build",
    "import-builds": "node src/script/import-builds.js",
    "test": "echo \"Future test will go here!\" && exit 0",
    "meta": "vimlet-meta -i src/meta/**/*.vmt -o build",
    "meta:watch": "vimlet-meta -i src/meta/**/*.vmt -o build -w src/meta",
    "copy": "vimlet-copy -i src/copy/**/* -o build",
    "copy:watch": "vimlet-copy -i src/copy/**/* -o build -w src/copy",
    "less": "vimlet-less -i src/less/**/* -o build",
    "less:watch": "vimlet-less -i src/less/**/* -o build -w src/less",
    "start": "node src/server/index.js",
    "release": "run-s release:core release:ui",
    "release:core": "cd eon-core && npm run release",
    "release:ui": "cd eon-ui && npm run release"
  },
  "dependencies": {
    "npm-run-all": "4.1.5",
    "express": "4.16.4",
    "@vimlet/copy": "0.0.8",
    "@vimlet/meta": "2.0.16",
    "@vimlet/less": "0.0.6"
  },
  "license": "MIT"
}
