<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title></title>
  <link rel="stylesheet"
       href="/example/ui/css/style.css">
</head>

<body>

  <span class="title">VimletComet</span>

  <ul>

    <% 
      var fs = require("fs");
      fs.readdirSync("./src/main/example/ui").sort().forEach(file => {
        if(file.endsWith(".html")){
          echo("<li><a href=\"/example/ui/" + file + "\"> " + file + "</a></li>\n");      
        }
      });    
      fs.readdirSync("./src/main/example/data").sort().forEach(file => {
        if(file.endsWith(".html")){
          echo("<li><a href=\"/example/data/" + file + "\"> " + file + "</a></li>\n");      
        }
      });    
      fs.readdirSync("./src/main/example/core/modules").sort().forEach(file => {
        if(file.endsWith(".html")){
          echo("<li><a href=\"/example/core/modules/" + file + "\"> " + file + "</a></li>\n");      
        }
      });    
    %>  

  </ul>

</body>

</html>
