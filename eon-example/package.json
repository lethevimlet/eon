{
  "name": "eon-example",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "run-s copy less meta",
    "build:watch": "run-p copy:watch less:watch meta:watch",
    "copy-builds": "node src/script/copy-builds.js",
    "watch-builds": "node src/script/watch-builds.js",
    "meta": "vimlet-meta -i src/meta/**/*.vmt -o build",
    "meta:watch": "vimlet-meta -i src/meta/**/*.vmt -o build -w src/meta",
    "copy": "vimlet-copy -i src/copy/**/* -o build",
    "copy:watch": "vimlet-copy -i src/copy/**/* -o build -w src/copy",
    "less": "vimlet-less -i src/less/**/* -o build",
    "less:watch": "vimlet-less -i src/less/**/* -o build -w src/less",
    "start": "run-p start:example start:doc",
    "start:example": "node src/server/index.js",
    "start:doc":"cd ../eon-docs/ && npm start"
  },
  "dependencies": {
    "@vimlet/copy": "0.0.12",
    "@vimlet/meta": "2.0.16",
    "@vimlet/less": "0.0.6",
    "@vimlet/run": "0.0.5",
    "npm-run-all": "4.1.5",
    "express": "4.16.4"
  },
  "license": "MIT"
}
