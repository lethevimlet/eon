{
  "api": [{
    "include": [
      "../eon-ui/src/copy/eon/**/*.html"
    ],
    "extension": false,
    "output": "release/api",
    "entry":"api",
    "version":"1.0.X",
    "compactPaths": ["ui/"],
    "folderIcon": false,
    "arrowIcon": true
  }],
  "article": [{
    "include": [
      "md/tutorial/**/*.md"
    ],
    "extension": false,
    "output": "release/tutorial",
