    "version":"1.0.X",
    "folderIcon": false,
    "arrowIcon": true
  }],
  "main": {
    "versions": {
      "1.0.X": {
        "latest": true,
        "entries": {
          "article": {
            "title": "Tutorial",
            "icon": "list",
            "brief": "Get Started"
          },
          "api": {
            "title": "API",
            "icon": "vertical-split",
            "brief": "API Documentation"
          }
        },
        "mainIcon": "logo.svg",
        "mainIconLink": "http://eonjs.org",
        "copy": {
          "include": [
            "copy"
          ]
        }
      }
    },
    "output": "release/framework/doc-eon"
  }
